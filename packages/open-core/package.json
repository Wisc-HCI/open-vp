    "framer-motion": "^9.1.6",
    "lodash": "^4.17.21",
    "number-precision": "^1.5.2",
    "react-icons": "^4.4.0",
    "react-use-measure": "^2.1.1",
    "ts-config-open-vp": "*",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "eslint": "8.15.0",
    "eslint-config-open-vp": "*",
    "react": "18.1.0",
    "ts-config-open-vp": "*",
    "tsup": "5.10.1"
