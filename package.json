{
  "name": "simple-vp",
  "version": "0.1.0",
  "main": "dist/index.js",
  "private": false,
  "files": [
    "dist",
    "README.md"
  ],
  "homepage": "http://Wisc-HCI.github.io/simple-vp",
  "repository": {
    "type": "git",
    "url": "https://github.com/Wisc-HCI/simple-vp.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@babel/cli": "^7.16.8",
    "@react-spring/web": "^9.4.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "grommet": "^2.20.0",
    "immer": "^9.0.12",
    "react": "^17.0.2",
    "react-dnd": "^14.0.5",
    "react-dnd-html5-backend": "^14.1.0",
    "react-dom": "^17.0.2",
    "react-flow-renderer": "^10.0.0-next.35",
    "react-icons": "^4.3.1",
    "react-scripts": "5.0.0",
    "react-spring": "^9.4.2",
    "styled-components": "^5.3.3",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.3",
    "zustand": "^4.0.0-beta.0"
  },
  "scripts": {
    "start": "npm run storybook",
    "build": "npm run build-storybook",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public && rm -rf docs && mv storybook-static docs",
    "clean": "rimraf dist",
    "compile": "npm run clean && cross-env BABEL_ENV=production babel src -d dist --ignore __tests__,spec.js,test.js,stories.js,__snapshots__",
    "generate-all": "npm run compile && npm run build-storybook"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.12",
    "@storybook/addon-essentials": "^6.4.12",
    "@storybook/addon-links": "^6.4.12",
    "@storybook/builder-webpack5": "^6.4.12",
    "@storybook/manager-webpack5": "^6.4.12",
    "@storybook/node-logger": "^6.4.12",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.12",
    "cross-env": "^7.0.3",
    "webpack": "^5.66.0"
  }
}
